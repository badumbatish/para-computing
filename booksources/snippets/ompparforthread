// parfor.c
#pragma omp parallel 
  {
    int
      nthreads = omp_get_num_threads(),
      thread_num = omp_get_thread_num();
    printf("Thread %d entering parallel region\n",nthreads);
    #pragma omp for
    for (int iter=0; iter<nthreads; iter++)
      printf("thread %d executing iter %d\n",
             thread_num,iter);
  }
